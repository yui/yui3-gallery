    "version": "0.1.3",
    "author": "Eugene Kashida",
    "contributors": [
        "Caridy PatiÃ±o"
    ],
    "dependencies": {},
    "locator": {
        "ruleset": "main",
        "location": "./js"
    },
    "keywords": [
        "yui",
